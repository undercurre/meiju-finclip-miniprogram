.active-dialog {
  position: fixed;
  right: 26rpx;
  bottom: calc(144rpx + env(safe-area-inset-bottom));
  z-index: 999; /* 协议更新时 遮罩层需要覆盖浮动窗口 */
}
.active-dialog-main {
  position: relative;
  width: 108rpx;
  height: 108rpx;
}
.active-dialog-main-click {
  position: absolute;
  left: -10rpx;
  right: -10rpx;
  top: -10rpx;
  bottom: -10rpx;
}
.close {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  right: -10rpx;
  top: -10rpx;
}
.close-click {
  position: absolute;
  width: 44rpx;
  height: 44rpx;
}
