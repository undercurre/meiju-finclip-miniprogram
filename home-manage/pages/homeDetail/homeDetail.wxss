/* home-manage/pages/homeDetail/homeDetail.wxss */
page {
  background-color: #f9f9f9;
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  --iphonex-fix-bottom: env(safe-area-inset-bottom);
}
.detail-container{
  margin-top: 20rpx;
}
.family-name{
  font-size: 28rpx;
  color: #666666;
  /* width: 280rpx; */
  text-overflow: ellipsis;
  vertical-align: middle;
  overflow: hidden;
  white-space: nowrap;
  font-weight: 400;
}
.member-container{
  margin: 24rpx 32rpx;
  background-color: #fff;
  border-radius: 32rpx;
  overflow: hidden;
  padding: 0 32rpx;
}
.member-head{
  padding: 40rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.member-head text{
  font-size: 32rpx;
  font-weight: 400;
  line-height: 1;
}
.avatar-container{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding-top: 32rpx;
  padding-bottom: 48rpx;
}
.avatar{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 120rpx;
  margin-left: 46rpx;
  overflow: hidden;
}
.avatar:nth-child(4n+1){
  margin-left: 0;
}
.avatar:nth-child(n+5){
  margin-top: 40rpx;
}
.avatar image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.avatar text{
  width: 100%;
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #999;
  font-weight: 400;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: center;
}
.avatar-box {
  position: relative;
  height: 120rpx;
}
.avatar-box text {
  position: absolute;
  left: 50%;
  bottom: 0;
  font-size: 18rpx;
  margin: 0 0 0 -46rpx;
  background-color: rgba(0, 0, 0, .7);
  color: #fff;
  width: 92rpx;
  line-height: 32rpx;
  border-radius: 18rpx;
  border: 1px solid #fff;
}
.add-icon{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  padding: 0;
  margin: 0;
  border: 1px solid #f2f2f2;
  overflow: hidden;
  background-repeat: no-repeat;
  background-size: 48rpx 48rpx;
  background-position: center center;
  background-color: #F9F9F9;
  outline: 0;
}
button::after{
  border: none;
}
.Ico{
  width: 24rpx;
  height: 24rpx;
}
.delete-btn-container{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 0 32rpx calc(var(--iphonex-fix-bottom));
  z-index: 1;
  /* background-color: #fff; */
  position: fixed;
}
.dialog-input{
  padding:32rpx 30rpx 20rpx;
}
.input-class{
  font-size: 24rpx;
}
.field-container{
  padding-bottom: 12rpx;
}
.tips-container{
  line-height: 1;
}
.tips{
  font-size: 20rpx;
  line-height: 20rpx;
  color: #FF8225;
}
.home-detil-Warp{
  position: relative;
  border-radius: 16px;
  background: #fff;
  margin: 20rpx 32rpx;
  padding: 36rpx 32rpx;
  box-sizing: border-box;
}
.home-detil-Warp .title {
  display: flex;
  align-items: center;
  line-height: 1;
  font-weight: 500;
  font-size: 32rpx;
}
.home-detil-Warp .numList {
  font-size: 24rpx;
  font-family: 'PingFang SC';
  color: #666666;
  display: flex;
  margin-top: 32rpx;
  align-items: center;
}

.home-detil-Warp .itemWarp {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  /* height: 24rpx; */
  padding-left: 12px;
  padding-right: 12px;
}

.home-detil-Warp .itemWarp::after {
  position: absolute;
  right: 0;
  top: 0;
  content: '';
  width: 1px;
  height: 200%;
  border-right: 1px solid #e4e4e4;
  transform-origin: 0 0;
  transform: scaleY(0.5);
}
.home-detil-Warp .itemWarp:first-child{
  padding-left: 0;
}
.home-detil-Warp .itemWarp:nth-child(3)::after {
  position: absolute;
  right: 0;
  top: 0;
  content: '';
  width: 0;
  height: 0;
}

.home-detil-Warp .Ico {
  width: 24rpx;
  height: 24rpx;
}
.home-detil-Warp  .centerDes {
  margin: 0 6rpx 0 12rpx;
}
.member-mange{
  font-size: 28rpx;
  color: #666666;
  line-height: 28rpx;
  font-weight: 400;
}
.member-mange-icon{
  margin-left: 8rpx;
}
.detail-container .mx-cell__title_title{
  font-weight: 400!important;
}
.delete-btn-container{
  --button-large-height:92rpx
}
.custom-button-class .mx-button__text{
  font-weight: 500!important;
}
.custom-button-class{
  margin-bottom: 24rpx;
}

.exit-dialog-class .mx-dialog__cancel-button .mx-button__text{
 font-weight: 500!important;
}

.exit-dialog-class .mx-dialog__confirm-button .mx-button__text{
  font-weight: 400!important;
 }