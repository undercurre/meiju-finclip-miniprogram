/* home-manage/pages/roomDetail/roomDetail.wxss */
page {
  background-color: #f9f9f9;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
  --iphonex-fix-bottom: env(safe-area-inset-bottom);
}
.main-container{
  margin-top: 20rpx;
  padding-bottom: calc(204rpx + var(--iphonex-fix-bottom));
}
.room-name{
  text-overflow: ellipsis;
  vertical-align: middle;
  overflow: hidden;
  white-space: nowrap;
  font-size: 24rpx;
  color: #8A8A8F;
  font-weight: 400;
}
.device-list-title{
  margin: 16rpx 0 16rpx 0;
  font-size: 28rpx;
  color: #999;
}
.btn-container{
  width: 100%;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 80rpx 32rpx calc(var(--iphonex-fix-bottom));
  background-color: #f9f9f9;
}
.dialog-input{
  padding:32rpx 30rpx 20rpx;
}
.input-class{
  font-size: 24rpx;
}
.field-container{
  padding-bottom: 12rpx;
}
.tips-container{
  line-height: 1;
}
.tips{
  font-size: 20rpx;
  line-height: 20rpx;
  color: #FF8225;
}
.custom-nav-class{
  --nav-bar-text-color:#000000;
  --nav-bar-text-margin:0 32rpx;
  --nav-bar-text-padding:0
}
.main-container .mx-cell__title_title{
  font-weight: 400!important;
}