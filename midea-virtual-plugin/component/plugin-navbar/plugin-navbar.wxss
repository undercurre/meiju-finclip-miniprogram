.top-headers {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  overflow: hidden;
}
.topHeaderBg {
  background: #fff;
}
.header {
  height: 60rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 16px;
  font-weight: 500;
  white-space: nowrap;
  position: absolute;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.logo {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  margin-right: 18rpx;
}

.back {
  position: absolute;
  left: 36rpx;
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
}
.back-click {
  position: absolute;
  left: 36rpx;
  width: 50rpx;
  height: 50rpx;
}

.title {
  display: flex;
  flex: 1;
  justify-content: center;
  font-size: 16px;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  /* font-weight: bold; */
}
