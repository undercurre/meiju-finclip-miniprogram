.fail-home {
  padding-top: 260rpx;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 180rpx;
  left: 0;
  /* background-image: linear-gradient(0deg, #f6f6f6 33%, #ecf0f6 100%); */
  z-index: 99;
}
.img-no-home {
  display: block;
  width: 320rpx;
  height: 320rpx;
  margin: 0 auto;
}
.fail-text {
  font-size: 28rpx;
  color: #8a8a8f;
  margin: 30rpx 0 56rpx;
  text-align: center;
}
.fail-btn {
  width: 200rpx;
  height: 60rpx;
  background: #0078ff;
  border-radius: 30rpx;
  font-size: 28rpx;
  color: #ffffff;
  line-height: 60rpx;
  margin: 0 auto;
  text-align: center;
}
