.swiper-dialog {
  background: #fff;
  border-radius: 40rpx 40rpx 0 0;
  position: absolute;
  width: 100%;
  height: 400rpx;
  left: 0;
  bottom: -40rpx;
  z-index: 99999;
}
.padding-b {
  padding: 32rpx;
  padding-bottom: 68rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.left_text {
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #232323;
  letter-spacing: 0;
  line-height: 28rpx;
  font-weight: 500;
}

.right-chacha {
  width: 32rpx;
  height: 32rpx;
  font-size: 32rpx;
}

.animimate-box {
  height: 128rpx;
  /* background:#ff0036; */
}

.flex-box {
  width: 100%;
  height: 128rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-content: center;
}
.isOne {
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: flex-start;
}
.only-one {
  width: 100rpx;
  height: 128rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-content: center;
  margin-right: 35rpx;
  margin-left: 35rpx;
}
.only-one:nth-child(1) {
  padding-left: 0rpx;
}
.only-one:last-child {
  margin-right: 0rpx;
}

.p-img {
  width: 96rpx;
  height: 96rpx;
}

.p-name {
  width: 100rpx;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #666666;
  letter-spacing: 0;
  /* line-height: 20rpx; */
  font-weight: 400;
  /* overflow: hidden; */
  text-overflow: ellipsis;
  white-space: nowrap;
}
.isLeftMargin:nth-child(1) {
  width: 134rpx;
  width: 100rpx;
  height: 128rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-content: center;
  margin-right: 37rpx;
  margin-left: 34rpx;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.active-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  /* width: 100%;
    height: 100%; */
  opacity: 0.6;
  z-index: 99999;
  background: #000000;
}
