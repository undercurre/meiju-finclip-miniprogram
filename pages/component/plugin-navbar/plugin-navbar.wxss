.header {
  height: 60rpx;
  width: 100%;
  padding-left: 32rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 18px;
  white-space: nowrap;
  position: fixed;
  top: 44rpx;
  left: 0;
  z-index: 99;
  display: flex;
  align-items: center;
}

.logo {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  margin-right: 18rpx;
}
.back {
  /* width: 34rpx;
  height: 34rpx; */
  width: 56rpx;
  height: 56rpx;
  display: inline-block;
}

.title {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #000000;
  text-align: center;
  font-weight: 500;
  width: 312rpx;
  word-break: keep-all;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.stop-temporarily{
  font-size: 32rpx;
  letter-spacing: 0;
  font-weight: 400;
}
