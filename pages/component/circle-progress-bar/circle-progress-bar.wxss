.container {
  box-sizing: border-box;
  position: relative;
}

.circle-background {
  box-sizing: border-box;
  border-radius: 50%;
}

.circle-progress {
  box-sizing: border-box;
  position: absolute;
  left: 0px;
  top: 0px;
}

.circle-progress.part1 {
  transform-origin: 100% 100%;
}

.circle-progress.part2 {
  transform-origin: 50% 100%;
}

.circle-progress.part3 {
  transform-origin: 50% 100%;
}

.circle-center-cover-wrapper {
  box-sizing: border-box;
  position: absolute;
  left: 0px;
  top: 0px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.circle-center-cover {
  border-radius: 50%;
}

.circle-bottom-cover {
  box-sizing: border-box;
  position: absolute;
  border-radius: 0 0 100% 0;
  transform-origin: 0% 0%;
  transform: rotate(45deg);
}

.desc {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #8a8a8f;
  letter-spacing: 0;
  line-height: 12px;
  width: 80rpx;
  text-align: center;
}

.min-desc {
  position: absolute;
}

.max-desc {
  position: absolute;
}

.slot-content {
  box-sizing: border-box;
  position: absolute;
  margin: auto;
  border-radius: 50%;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
}
