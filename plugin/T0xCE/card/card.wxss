.row-sb {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.container {
    background: #fff;
    border-radius: 10rpx;
    width: 100%;
    height: 100%;
    position: relative;
}

.flex-floor {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.flex-floor-top {
    /* height: 100%; */
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.blank-floor{
    height: 128rpx;
}

