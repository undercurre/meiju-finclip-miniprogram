.container {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  margin: 16px 5% 30px 5%;
  background: transparent;
  left: 0;
  right: 0;
}
.list-header-content {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 44px;
  align-items: center;
}
.goto-insurance-desc {
  color: #818181;
  font-size: 12px;
  text-align: right;
  margin-inline-start: auto;
}
.hover-class {
  display: flex;
  flex-direction: row;
  margin-top: 5px;
  width: 100%;
  height: 76px;
  align-items: center;
  justify-items: center;
}
.notUsed {
  /* background-color: #FCF3D8;
    border-width: 1px;
    border-color: #FFBA00; */
  background-image: url('http://ismart.zhinengxiyifang.cn/midea_json_new/activity/bg_insurance_card_unused.png');
  background-size: 100% 100%;
}
.used {
  /* background-color: #EAEEFE;
    border-width: 0.5px;
    border-color: #4F72F7; */
  background-image: url('http://ismart.zhinengxiyifang.cn/midea_json_new/activity/bg_insurance_card_used.png');
  background-size: 100% 100%;
}
.expired {
  /* background-color: #ECECEC;
    border-width: 0.5px;
    border-color: #ACAAA9; */
  background-image: url('http://ismart.zhinengxiyifang.cn/midea_json_new/activity/bg_insurance_card_expired.png');
  background-size: 100% 100%;
}
.item-icon {
  position: absolute;
  left: 0;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100%;
  z-index: -999;
}
.item-title-desc {
  display: flex;
  flex-direction: column;
  margin-left: 5%;
  /* align-items: center; */
  justify-items: center;
}
.item-title {
  font-size: 16px;
  color: #2e2e2e;
  text-align: left;
}
.item-date {
  font-size: 12px;
  color: #666666;
  text-align: left;
}
.item-gap-line {
  width: 1px;
  height: 90%;
  background-color: white;
}
.item-status {
  width: 25%;
  align-items: center;
  justify-items: center;
  margin-inline-start: auto;
}
.item-status {
  font-size: 14px;
  color: #666666;
  text-align: center;
}
