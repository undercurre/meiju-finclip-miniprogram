.row-fs{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}
.row-sb{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.row-sa{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
}
.container {
    background: #fff;
    border-radius: 10rpx;
    width: 100%;
    height: 100%;
    position: relative;
}
.header-floor{
    position: absolute;
    height: 128rpx;
    z-index: 2;
    left: 0;
    right: 0;
    padding: 0 20rpx;
    background: #ffffff;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}
.scroll-floor{
    height: 100%;
}
.scroller-flex-floor{
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.blank{
  height: 128rpx;
}
.center-floor{
    /* padding-top: 190rpx; */
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    min-height: 406rpx;
}
.device-icon{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.device-name{
    font: bold 32rpx normal;
}
.device-room{
    margin-top: 4rpx;
    font-size: 26rpx;
    color: #666;
}
.footer-floor{
    padding-bottom: 10rpx;
}
.button-floor {
    margin-top: 0rpx;
    padding: 0 30rpx 30rpx;
    background: #fff;
}

/* tab ---- */
.eq-header {
    position: relative;
    text-align: center;
    height: 44rpx;
}
.tab-header {
    margin: 10px auto;
    display: flex;
    color: #666;
    position: relative;
    width: 160rpx;
}
.tab-header-card {
    flex:1;
    margin-bottom: -1rpx;
    text-align: center;
    /* border: 1rpx solid #efefef; */
    /* line-height: 180%; */
    font-size: 24rpx;
    height: 44rpx;
    line-height: 44rpx;
    background-color: #efefef;
    overflow: hidden;
}
.tab-header-card:first-child {
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
}
.tab-header-card:last-child {
    border-top-right-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}
.tab-header-card.current {
    color: #fff;
    /* color: rgb(255, 115, 0); */
    border-color: #FFBB00;
    background-color: #FFBB00;
    /* background-color: rgb(255, 248, 227); */
}

.tab-header-middle {
    margin: 10px auto;
    display: flex;
    color: #666;
    position: relative;
    width: 240rpx;
}

.baby-lock {
    position: absolute;
    right: 24rpx;
    top: 80%;
    transform: translateY(-50%);
    font-size: 24rpx;
    text-align: center;
    color: #666;
    vertical-align: middle;
}
.baby-lock image {
    width: 32rpx;
    height: 32rpx;
    vertical-align: bottom;
}

/* clock */
.clock-panel {
    position: relative;
    width: 686rpx;
    height: 474rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    text-align: center;
    align-items: center;
}
.scale {
    width: 2rpx;
    height: 30rpx;
    background-color: #ddd;
    position: absolute;
    bottom: 50%;
    left: 50%;
    margin-left: -1px;
    transform-origin: bottom;
    z-index: 1;
    /* transform: rotate(0deg) translateY(-250px); */
}
.eq-power-on .scale {
    background-color: rgb(255, 115, 0);
}
.circle {
    width: 320rpx;
    height: 320rpx;
    border-radius: 50%;
    border: 2rpx solid #eee;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    text-align: center;
    align-items: center;
}
.mainText {
    position: relative;
}
.mainText .number {
    font-size: 96rpx;
    position: relative;
    top: 15rpx;
}
.mainText .unit {
    position: absolute;
    left: 100%;
    top: 50rpx;
    font-size: 24rpx;
    color: #666;
}
.descText {
    font-size: 24rpx;
    text-align: center;
    color: #666;
}
.temperatureText {
    position: relative;
}
.temperatureText .number {
    font-size: 96rpx;
    position: relative;
}
.temperatureText .cook {
    position: relative;
}
.temperatureText .unit {
    position: absolute;
    left: 100%;
    top: 50rpx;
    font-size: 24rpx;
    color: #666;
}

.sub-button-floor{
    position: relative;
    border-top: 1rpx solid #EBEBEC;
    padding: 30rpx 10rpx;
    background: #FAFAFA;
}

.grey-triangle{
    position: absolute;
    width: 35rpx;
    height: 20rpx;
    top: -19rpx;
}
.grey-triangle.tri-wind{
    /* left: calc(37.5% - 17.5rpx); */
    left: 36.25%;
    /* left: calc(50% - 17rpx); */
}

.sub-button {
    width: 105rpx;
}
.sub-button + .sub-button{
    margin-left: 30rpx;
}


