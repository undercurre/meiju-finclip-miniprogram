.header {
  height: 60rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 18px;
  white-space: nowrap;
  position: fixed;
  top: 44rpx;
  left: 0;
  z-index: 99;
  display: flex;
  align-items: center;
  justify-content: center;
}

.logo {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  margin-right: 18rpx;
}
.back {
  position: absolute;
  left: 36rpx;
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
}
.title {
  display: flex;
  flex: 1;
  justify-content: center;
  font-size: 18px;
  font-weight: bold;
}
