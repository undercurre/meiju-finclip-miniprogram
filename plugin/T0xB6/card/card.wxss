.row-fs{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}
.row-sb{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.row-sa{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
}
.container {
    /* background-color: #F9F9F9; */
    background: #fff;
    border-radius: 10rpx;
    width: 100%;
    height: 100%;
    position: relative;
}
.header-floor{
    position: absolute;
    height: 128rpx;
    z-index: 2;
    left: 0;
    right: 0;
    padding: 0 20rpx;
    background: #ffffff;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}
.scroll-floor{
    height: 100%;
}
.scroller-flex-floor{
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.blank{
  height: 128rpx;
}
.center-floor{
    /* padding-top: 190rpx; */
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    min-height: 406rpx;
}
.device-icon{
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.device-name{
    font: bold 32rpx normal;
}
.device-room{
    margin-top: 4rpx;
    font-size: 26rpx;
    color: #666;
}
.footer-floor{
    padding-bottom: 10rpx;
}
.button-floor {
    margin-top: 0rpx;
    padding: 0 30rpx 30rpx;
    background: #fff;
}
.grey-triangle{
    position: absolute;
    width: 35rpx;
    height: 20rpx;
    top: -19rpx;
}
.grey-triangle.tri-wind{
    /* left: calc(37.5% - 17.5rpx); */
    left: 36.25%;
    /* left: calc(50% - 17rpx); */
}
.sub-button-floor{
    position: relative;
    border-top: 1rpx solid #EBEBEC;
    padding: 30rpx 10rpx;
    background: #FAFAFA;
}
.mode-floor{
    padding: 30rpx;
}
.wind-speed-floor{
    padding: 30rpx 100rpx
}

/* clock */
.clock-panel {
    position: relative;
    width: 686rpx;
    height: 474rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    text-align: center;
    align-items: center;
}
.scale {
    width: 2rpx;
    height: 30rpx;
    background-color: #ddd;
    position: absolute;
    bottom: 50%;
    left: 50%;
    margin-left: -1px;
    transform-origin: bottom;
    z-index: 1;
    /* transform: rotate(0deg) translateY(-250px); */
}
.eq-power-on .scale {
    background-color: #627BFF;
}
.circle {
    width: 320rpx;
    height: 320rpx;
    border-radius: 50%;
    border: 2rpx solid #eee;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    text-align: center;
    align-items: center;
}

.circle-progress-bar {
    position: relative;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    width: 470rpx;
    height: 470rpx;
}
.center-content{
    position: relative;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    align-items: center;
    text-align:center;
    width:400rpx;
    height:400rpx;
    border-radius: 50%;
    /* box-shadow: 0 0 15px 6px #5D75F6; */
    box-shadow: 0 0 15px 0 #E6EAEF;
}
.center-content-on{
    box-shadow: -0 0 4px 3px rgba(98, 123, 255, .5);
}
.center-content-left{
    box-shadow: -3px 0 7px 3px rgba(98, 123, 255, .5);
}
.center-content-right{
    box-shadow: 3px 0 7px 3px rgba(98, 123, 255, .5);
}
.center-content-off{
    box-shadow: 0 0 15px 6px #e6eaef;
}
.center-desc {
    position: absolute;
    bottom: 85rpx;
    left: 0rpx;
    width: 400rpx;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #666;
    letter-spacing: 0;
    line-height: 12px;
}
.descText {
    font-size: 24rpx;
    text-align: center;
    color: #666;
}
.appliance-gear-name {
    font-family: PingFangSC-Regular;
    font-size: 30px;
    color: #000000;
    letter-spacing: 0;
    text-align: center;
    line-height: 40px;
    width: 100%;
}
.appliance-off{
    font-family: PingFangSC-Regular;
    font-size: 36px;
    color: #000000;
    letter-spacing: 0;
    text-align: center;
    line-height: 40px;
    width: 100%;
}
.temp-btn{
    font-size: 32px;
    width: 32px;
    text-align: center;
    padding: 20px 8px;
    color: #5D75F6;
}
.appliance-temp{
    font-family: Roboto-Regular;
    flex: 1;
    font-size: 72px;
    color: #000000;
    letter-spacing: 0;
    text-align: center;
}
.temperature-desc{
    margin: 10rpx 0 84rpx;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #8A8A8F ;
    letter-spacing: 0;
    text-align: center;
}
.temperature-desc-value{
    font-family: Roboto-Regular;
    font-size: 16px;
    letter-spacing: 0;
    margin-left: 5rpx;    
}

