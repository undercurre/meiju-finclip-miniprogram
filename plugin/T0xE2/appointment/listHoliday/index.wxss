@import "../../assets/css/public.wxss";
page {
  background: #f9f9f9;
}

.content {
  padding-top: 16rpx;
  box-sizing: border-box;
  position: relative;
}

.none-sub {
  height: 100%;
  padding-top: 306rpx;
}

.none-sub .none-sub-img {
  width: 396rpx;
  height: 254rpx;
}

.none-sub .none-sub-text {
  font-size: 28rpx;
  color: #8a8a8f;
  margin-top: 48rpx;
}

.footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 128rpx;
  background: #fff;
}

.footer .footer-text {
  font-size: 32rpx;
  color: #ffaa10;
}

.cell {
  display: flex;
  align-items: center;
  width: 100% !important;
  height: 160rpx;
}

.cell .cell-title {
  font-family: PingFangSC-Regular;
  font-size: 32px;
  color: #000000;
}
.delete-btn {
  width: 160rpx;
  height: 160rpx;
  background: #ee0a24;
}

.delete-btn-text {
  color: #ffffff;
}
.list-item-title .day-desc {
  width: 55rpx;
  height: 45rpx;
  background: #ffaa10;
  border-radius: 10rpx;
  margin-right: 20rpx;
}

.list-item-title .day-desc .text {
  font-size: 20rpx;
  color: #fff;
}

.list-item-title .time {
  font-size: 32rpx;
  color: #000000;
}

.list-item-title .tem {
  font-size: 32rpx;
  color: #000000;
  margin-left: 30rpx;
}

.dialog .weui-dialog {
  width: 82%;
  transform: translateY(-50%) !important;
}

.appoint-tips{
  position: absolute;
  padding-left: 32rpx;
  padding-right: 32rpx;
  bottom: 168rpx;
}
.appoint-tips-desc {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  line-height: 32prx;
  opacity: 0.4;
  margin: 20rpx;
}
