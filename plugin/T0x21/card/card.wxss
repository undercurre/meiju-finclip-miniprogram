.button-text{
  font-size:24rpx;
  color:#232323;
}
.bg-img{
  position:absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size:72rpx;
  color:#000000;
}

button::after {
  border: none
}