.doorLock-img {
  width: 186rpx;
  height: 391rpx;
  transform: translate(25%, 0);
}

.temPassword-img {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 300rpx;
  height: 400rpx;
  transform: translate(-50%, -50%);
}

.container {
  text-align: center;
  position: relative;
}

.flex-floor {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.center-floor {
  position: relative;
}

.blank-floor {
  height: 128rpx;
}

.footer-floor {
  /* padding:0 44rpx; *//* padding: 10rpx 0; */
}

.button-floor {
  padding-bottom: 22rpx;
  text-align: center;
  background: #fff;
}

.button-floor-flex {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}

.button-icons {
  position: relative;
  height: 120rpx;
  width: 120rpx;
  border-radius: 50%;
  background-color: #eaeefe;
}

.button-img {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

