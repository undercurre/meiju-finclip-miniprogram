.header {
  /*height: 60rpx;*/
  width: 100%;
  padding-left: 32rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 18px;
  white-space: nowrap;
  position: fixed;
  top: 0;
  /*top: 44rpx;*/
  left: 0;
  z-index: 99;
  display: flex;
  align-items: center;
  background: #f9f9f9;
  padding-bottom: 10rpx;
}

.logo {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  margin-right: 18rpx;
}
.back {
  width: 34rpx;
  height: 34rpx;
  display: inline-block;
}
.center-title {
  width: 69%;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  padding-left: 15%;
  box-sizing: border-box;
}
