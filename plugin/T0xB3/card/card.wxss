@import "../assets/css/public.wxss";

page {
    background: #f9f9f9;
}

.continer {
    width: 100%;
    height: 100%;
}

.container-box {
  width: 100vw;
  overflow-x: hidden;
}

.cell {
    display: flex;
    align-items: center;
    width: 686rpx !important;
    height: 144rpx;
  }
  
  .cell-value {
    font-size: 32rpx;
    color: #666666;
  }
  
  .cell-img-wrap {
    width: 64rpx;
    height: 64rpx;
    border-radius: 32rpx;
    margin-right: 32rpx;
  }
  
  .cell-img-wrap .cell-img {
    width: 64rpx;
    height: 64rpx;
  }
