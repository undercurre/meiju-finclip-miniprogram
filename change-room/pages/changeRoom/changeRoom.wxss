/* change-room/pages/changeRoom/changeRoom.wxss */
.move-device-wrap{
  padding-top: 16rpx;
}
page{
  background-color: #f9f9f9;
}

.changeRoomWarp {
  /* position: fixed; */
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: #f9f9f9;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}

.content {
  background: #fff;
  margin-top: 32rpx;
}

.greyBar {
  width: 100%;
  height: 32rpx;
}

.list {
  width: 100%;
  height: 112rpx;
  display: flex;
  align-items: center;
  width: 718rpx;
  margin-left: 32rpx;
  border-bottom: 1px solid #F2F2F2;
  justify-content: space-between;
}

.list:last-child {
  border-bottom: none;
}

.roomName {
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #000000;
  letter-spacing: -0.78px;
  font-weight: 500;
}

.chooseIco {
  width: 32rpx;
  height: 32rpx;
  margin-right: 32rpx;
}

.home-list-show{
  /* //margin-top: 16rpx; */
}
.home-select-item{
  position: relative;
  margin: 0 32rpx;
  height: 104rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.home-select-item:not(:last-child):after{
  content: '';
  position: absolute;
  bottom: 0;
  background: #F2F2F2;
  width: 100%;
  height: 1px;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}
.custom-nav-class{
  --nav-bar-text-color:#000000;
  --nav-bar-text-margin:0 32rpx;
  --nav-bar-text-padding:0
}