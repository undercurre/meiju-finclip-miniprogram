/* 返回app按钮 */
.open-app {
  position: fixed;
  z-index: 1000;
  bottom: 70px;
  left: 50%;
  transform: translateX(-50%);
  height: 35px;
  width: 100px;
  border-radius: 15px;
  background-color: #267aff;
  text-align: center;
  line-height: 35px;
  font-size: 14px;
  color: #fff;
}

.content {
  width: 100%;
  overflow: auto;
  height: auto;
  background: #f9f9f9;
  font-family: PingFangSC-Regular;
  -webkit-overflow-scrolling: touch; 
}

.content::-webkit-scrollbar {
  display: none;
}

.personal {
  padding-bottom: 48rpx;
  background-color: #fff;
  margin-bottom: 16rpx;
  box-sizing: border-box;
}
