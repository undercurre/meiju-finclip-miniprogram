/* -------文章---------- */

.article {
  padding: 28rpx 0 0px 32rpx;
  box-sizing: border-box;
}

.article > .title {
  font-size: 18px;
  color: #000;
  letter-spacing: 0;
  line-height: 1.4;
  font-weight: 700;
}

.article-text {
  padding: 14px 0px;
  font-size: 14px;
  color: #000;
  letter-spacing: 10;
  line-height: 1.4;
}

.fload {
  position: relative;
  margin-top: 12rpx;
  height: auto;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 5;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #333;
  letter-spacing: 10;
  line-height: 40rpx;
}

.expand-article {
  background-color: #fff;
  font-size: 12px;
  color: #267aff;
  letter-spacing: 0;
  line-height: 1.4;
  text-align: left;
}

.hide {
  overflow: hidden;
}

.show {
  display: block;
  text-overflow: clip;
  overflow: visible;
}

/* ----------标签 ---------- */

.label {
  width: 100%;
