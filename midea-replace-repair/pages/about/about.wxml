<!--midea-replace-repair/pages/about/about.wxml-->
<view class="conrainer-wrap">
  <view class="title">{{title}}</view>
  <view wx:for="{{describeText}}" wx:key="*this" wx:for-item="item" class="item">
    <view class="sub-title">{{item.subTitle}}</view>
    <view wx:for="{{item.content}}" wx:key="*this" wx:for-item="miniItem">
      <text class="content-text">{{miniItem}}</text>
    </view>
  </view>
</view>
