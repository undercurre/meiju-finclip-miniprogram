<view class="continer">
  <mx-nav-bar title="{{title}}" show-bg="{{isHideNavBg}}" bind:click-left="onClickLeft" bgColor="#FFFFFF" title-class="title-class"/>
  <view class="device-card">
    <device-card
      wx:if="{{isInit}}"
      class="component{{deviceInfo.applianceCode}}"
      applianceData="{{deviceInfo}}"
    ></device-card>
  </view>
</view>
<midea-toast id="media-toast"></midea-toast>

