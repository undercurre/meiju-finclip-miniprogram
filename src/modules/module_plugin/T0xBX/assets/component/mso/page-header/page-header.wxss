.header {
  height: 60rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 18px;
  white-space: nowrap;
  position: fixed;
  top: 44rpx;
  left: 0;
  z-index: 102;
  display: flex;
  align-items: center;
  justify-content: center;
}
.logo {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  margin-right: 18rpx;
}
.back-button {
  position: absolute;
  left: 32rpx;
  width: 58rpx;
  height: 58rpx;
}
.back-button .button-icon {
  width: 58rpx;
  height: 58rpx;
}
.header-text_light,
.header-text_dark {
  font-size: 32rpx;
}
.header-text_light {
  color: #000000;
}
.header-text_dark {
  color: #fff;
}
