.click:active{
    opacity: .6;
}
.center-flex {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  /* z-index: 1; */
}
.start-flex{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.between-flex{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.transition-default{
    transition: all ease-in-out .3s;
}
.fade-out{
    opacity: 0;
}
.fade-in{
    opacity: 1;
}
/* 小木马 */
.hourseWarp{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: #F9F9F9;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    z-index: 89;
}
.swing {
    margin: 0 auto;
    width:128rpx;
    height:128rpx;
}
