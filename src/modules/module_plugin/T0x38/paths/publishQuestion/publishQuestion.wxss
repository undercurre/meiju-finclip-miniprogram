/* plugin/T0xB8/cards/X9/paths/publishQuestion/publishQuestion.wxss */
page {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.background-image {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
}
.container {
  position: relative;
  width: 100%;
  height: calc(100% - 142rpx);
  overflow-y: scroll;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background-color: #0F0F24;
}