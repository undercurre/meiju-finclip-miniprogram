/* plugin/T0xB8/cards/X9/paths/material/material.wxss */
.container {
  box-sizing: border-box;
  position: relative;
  width: 100%;
  height: 100vh;
  overflow-y: scroll;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  background-color: #0F0F24;
}
.item {
  box-sizing: border-box;
  height: 220rpx;
  background-color: rgba(255, 255, 255, 0.1);
  border-radius: 16rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 24rpx 32rpx 0 32rpx;
  padding: 40rpx 34rpx 40rpx 30rpx;
  position: relative;
}
/* item图标 */

.list-icon {
  width: 208rpx;
  height: 140rpx;
}
/* item 中间内容 */
.des {
  text-align: left;
  position: absolute;
  left: 268rpx;
  top: 48rpx;
}
.num {
  display: flex;
  align-items:flex-end;
  color: #0091FF;
}
.value {
  font-family: PingFang-SC-Medium;
  font-size: 48rpx;
  line-height: 48rpx;
  font-weight: 500;
}
.unit {
  font-family: PingFang-SC-ExtraLight;
  font-size: 24rpx;
  line-height: 48rpx;
  font-weight: 400;
}
.name {
  margin-top: 8rpx;
  font-family: PingFang-SC-Bold;
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 32rpx;
  font-weight: 700;
}
.des-text {
  margin-top: 14rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #8A8A8F;
  line-height: 24rpx;
  height: 24rpx;
  font-weight: 400;
}
/* item 购买跳转 */
.right {
  display: flex;
  align-items: center;
}
.right-text {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #8A8A8F;
  text-align: right;
  line-height: 34rpx;
  height: 34rpx;
  font-weight: 400;
  margin-right: 20rpx;
}
.right-icon {
  width: 16rpx;
  height: 16rpx;
  transform: rotate(45deg);
  border-top: 3rpx solid rgba(200,199,204,1);
  border-right: 3rpx solid rgba(200,199,204,1);
}
/* item 提示框 */
.tip {
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
}
/* 重置区域 */
.reset-wrap {
  position: absolute;
  top: 80%;
  width: 100%;
}
.note {
  text-align: center;
  height: 28rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: rgba(255,255,255,0.70);
  line-height: 28rpx;
  font-weight: 400;
}
.btn {
  margin-top: 30rpx;
  margin-left: 100rpx;
  width: 550rpx;
  height: 88rpx;
  background: #267AFF;
  border-radius: 200rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
  line-height: 88rpx;
  font-weight: 500;
}
/* 重置弹框 */
.popup-title {
  height: 102rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #000000;
  letter-spacing: 0;
  text-align: center;
  line-height: 102rpx;
  font-weight: 500;
  border-bottom: 1rpx solid #F2F2F2;
}
.checkbox-group {
  background: #F2F2F2;
  margin: 35rpx 24rpx 50rpx 24rpx;
  border-radius: 16rpx;
}
.cell {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 102rpx;
  padding: 0 34rpx 0 32rpx;
}
.cell-title {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  color: #000000;
  font-weight: 400;
  text-align: left;
}
.checkbox-line {
  margin-left: 5%;
  width: 95%;
  height: 2rpx;
  background: #D8D8D8;
}
.popup-btn {
  margin: 0 32rpx 94rpx;
  height: 92rpx;
  background: #267AFF;
  border-radius: 48rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 92rpx;
  font-weight: 500;
}
/* 维护保养弹框 */
.video-popup {
  position: relative;
}
.close-popup-icon {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: 36rpx;
  right: 28rpx;
}
.video-cell {
  display: flex;
  box-sizing: border-box;
  position: relative;
  margin: 24rpx 32rpx 0 32rpx;
  padding: 16rpx 0;
  height: 212rpx;
  background: #FFFFFF;
  box-shadow: 0 4rpx 16rpx 0 rgba(0,0,0,0.15);
  border-radius: 24rpx;
}
.cell-first {
  background: rgba(0,0,0,0.15);
  margin-top: 40rpx;
}
.video-poster {
  width: 240rpx;
  height: 180rpx;
  margin: 0 32rpx 0 16rpx;
  background: #565665;
  border-radius: 16rpx;
}
.video-cell-text {
  margin-top: 14rpx;
}
.video-cell-title {
  height: 36rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #000000;
  line-height: 36rpx;
  font-weight: 500;
  text-align: left;
}
.video-cell-time {
  margin-top: 24rpx;
  height: 24rpx;
  font-family: DINAlternate-Bold;
  font-size: 24rpx;
  color: #666666;
  line-height: 24rpx;
  font-weight: 700;
  text-align: left;
}
.video-cell-audience {
  margin-top: 46rpx;
  height: 20rpx;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #8A8A8F;
  line-height: 20rpx;
  font-weight: 400;
  text-align: left;
}
.know-more {
  width: 112rpx;
  height: 48rpx;
  position: absolute;
  bottom: 20rpx;
  right: 28rpx;
}