                        <image src="{{iconServiceUrl}}loading.png" style="margin-left:16rpx;" mode="aspectFill" class="animation-loading-view"></image>
                      </view>
                    </view>
                    <view style="width:211rpx;" class="row-end-container" wx:if="{{rightBwsZoneModel.canClose}}">
                      <!-- 开状态 -->
                      <view class="row-end-container ai-opt-ctr-switch-open-out" wx:if="{{rightBwsZoneModel.switch == 'on'}}" bindtap="closeCtlSwitchAction" data-id="ybws">
                        <view class="ai-opt-ctr-switch-open-in"></view>
                      </view>
                      <!-- 关状态 -->
                      <view class="row-start-container ai-opt-ctr-switch-close-out" wx:if="{{rightBwsZoneModel.switch == 'off'}}" bindtap="openCtlSwitchAction" data-id="ybws">
                        <view class="ai-opt-ctr-switch-close-in"></view>
                      </view>

                    </view>
                  </view>
                </view>
                <!-- 调节控件  -->
                <view style="height:50rpx;opacity:{{rightBwsZoneModel.switch == 'off'?0.3:1}}" class="column-center-container container-common-width">
                  <slider disabled="{{rightBwsZoneModel.switch == 'off'?true:false}}" min="0" max="{{rightBwsZoneModel.gearList.length - 1}}" step="1" value="{{rightBwsZoneModel.iCurValue}}" block-size="20" block-color="#267AFF" backgroundColor="#F2F2F2" activeColor="#267AFF" bindchange="sliderChangeAction" bindchanging="sliderChangingAction" data-id="ybws"></slider>
                </view>
                <!-- 数值显示 -->
                <view style="height:24rpx;opacity:{{rightBwsZoneModel.switch == 'off'?0.3:1}}" class="row-between-container container-common-width">
                  <view class="temperature-value" wx:for="{{rightBwsZoneModel.gearList}}">{{item.gearName}}</view>
                </view>
              </view>
            </view>
            <!-- 右变温室 档位调节 结束 -->
          </view>
        </view>
      </view>
      <!-- 内容结束 -->
    </view>

  </block>
  <block wx:else>
    <offline></offline>
  </block>
</view>

