<view class="container">
  <block wx:if="{{onlineStatus == 1}}">
    <!--调温冰箱-->
    <!-- 内容区域：分为中部和底部两个区域，使用flex布局，底部区域固定在card底部 -->
    <view class="column-start-container">
      <!--使flex生效，不可删除此包裹元素-->
      <!--  内容主体 -->
      <view class="column-start-container" style="margin-bottom:30rpx;">
        <!-- 冷冻室、温度显示位置 -->
        <view class="column-center-container temperature-show-view">
          <view class="circular-out-shadow">
            <view class="circular-in-shadow column-center-container">
              <!-- 温度设置 -->
              <view wx:if="{{fridgeZoneType == 1 && !devChecking}}" class="fridge-zone-type-box-1 column-center-container">
                <!-- 冷藏室、冷冻室 -->
                <view class="column-center-container temperature-show-space">
                  <view class="row-center-container temperature-show-space-row" style="margin-bottom: 30rpx;">
                    <!-- 冷藏室 -->
                    <view class="column-center-container">
                      <view style="font-size: 24rpx;color: #7C879B;">{{lcsZoneModel.bingWenModel}}</view>
                      <view style="margin-top: 14rpx;display: flex;align-items: flex-end;min-width: 118rpx;">
                        <text style="height:80rpx;width: 70rpx;font-size: 62rpx;color: #000000;font-weight: 500;text-align: right;line-height: 80rpx;">{{lcsZoneModelCurValue}}</text>
                        <image wx:if="{{lcsZoneModel.showDegree&&lcsZoneModelCurValue!='一'}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                      </view>
                    </view>
                    <!-- 中线 -->
                    <view class="column-center-container halving-line-contain" style="margin-left: 22rpx;margin-right: 18rpx;">
                      <view class="halving-line"></view>
                    </view>
                    <!-- 冷冻室 -->
                    <view style="display: flex;align-items: flex-end;flex-direction: row;">
                      <view class="column-end-container" style="align-items: flex-end;">
                        <view style="font-size: 24rpx;color: #7C879B;">{{ldsZoneModel.bingWenModel}}</view>
                        <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:if="{{isDeepColdMode}}">{{deepcold_temp}}</text>
                        <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:else>{{ldsZoneModelCurValue}}</text>
                      </view>
                      <image wx:if="{{ldsZoneModel.showDegree}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                    </view>
                  </view>
                </view>
              </view>
              <view wx:if="{{fridgeZoneType == 2&& !devChecking}}" class="fridge-zone-type-box-2 column-start-container">
                <!-- 冷藏室、冷冻室 -->
                <view class="column-start-container temperature-show-space">
                  <view style="margin-top:90rpx;" class="row-start-container temperature-show-space-row">
                    <view class="row-center-container temperature-show-space-row">
                      <!-- 冷藏室 -->
                      <view class="column-center-container">
                        <view style="font-size: 24rpx;color: #7C879B;">{{lcsZoneModel.bingWenModel}}</view>
                        <view style="margin-top: 14rpx;display: flex;align-items: flex-end;">
                          <text style="height:80rpx;width: 70rpx;font-size: 62rpx;color: #000000;font-weight: 500;text-align: right;line-height: 80rpx;">{{lcsZoneModelCurValue}}</text>
                          <image wx:if="{{lcsZoneModel.showDegree&&lcsZoneModelCurValue!='一'}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                        </view>
                      </view>
                      <!-- 中线 -->
                      <view class="column-center-container halving-line-contain" style="margin-left: 22rpx;margin-right: 18rpx;">
                        <view class="halving-line"></view>
                      </view>
                      <!-- 冷冻室 -->
                      <view style="display: flex;align-items: flex-end;flex-direction: row;">
                        <view class="column-end-container" style="align-items: flex-end;">
                          <view style="font-size: 24rpx;color: #7C879B;">{{ldsZoneModel.bingWenModel}}</view>
                          <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:if="{{isDeepColdMode}}">{{deepcold_temp}}</text>
                          <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:else>{{ldsZoneModelCurValue}}</text>
                        </view>
                        <image wx:if="{{ldsZoneModel.showDegree}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                      </view>

                    </view>
                  </view>
                </view>
                <!-- 变温室 -->
                <view style="width:420rpx;height:190rpx;" class="column-start-container">
                  <view style="width:420rpx;height:5rpx;" class="row-center-container">
                    <view style="width:276rpx;background: #E5E5E8;height:2rpx;"></view>
                  </view>
                  <view style="width:420rpx;height:24rpx;margin-top:24rpx;" class="row-center-container">
                    <view style="font-size:20rpx;color:rgba(138,138,144,0.75);font-weight:400;">{{bwsZoneModel.bingWenModel}}</view>
                  </view>
                  <view style="width:420rpx;height:30rpx;margin-top:20rpx;" class="row-center-container">
                    <text style="font-size:24rpx;color: #666666;font-weight:400;width:320rpx;text-align:center;">{{bwsZoneModelCurValue}}</text>
                  </view>
                </view>
              </view>
              <view wx:if="{{fridgeZoneType == 3 && !devChecking}}" class="fridge-zone-type-box-3">
                <!-- 冷藏室、冷冻室 -->
                <view class="column-start-container temperature-show-space">
                  <view style="margin-top:90rpx;" class="row-start-container temperature-show-space-row">
                    <view class="row-center-container temperature-show-space-row">
                      <!-- 冷藏室 -->
                      <view class="column-center-container">
                        <view style="font-size: 24rpx;color: #7C879B;">{{lcsZoneModel.bingWenModel}}</view>
                        <view style="margin-top: 14rpx;display: flex;align-items: flex-end;min-width: 118rpx;">
                          <text style="height:80rpx;width: 70rpx;font-size: 62rpx;color: #000000;font-weight: 500;text-align: right;line-height: 80rpx;">{{lcsZoneModelCurValue}}</text>
                          <image wx:if="{{lcsZoneModel.showDegree&&lcsZoneModelCurValue!='一'}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                        </view>
                      </view>
                      <!-- 中线 -->
                      <view class="column-center-container halving-line-contain" style="margin-left: 22rpx;margin-right: 18rpx;">
                        <view class="halving-line"></view>
                      </view>
                      <!-- 冷冻室 -->
                      <view style="display: flex;align-items: flex-end;flex-direction: row;">
                        <view class="column-end-container" style="align-items: flex-end;">
                          <view style="font-size: 24rpx;color: #7C879B;">{{ldsZoneModel.bingWenModel}}</view>
                          <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:if="{{isDeepColdMode}}">{{deepcold_temp}}</text>
                          <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:else>{{ldsZoneModelCurValue}}</text>
                        </view>
                        <image wx:if="{{ldsZoneModel.showDegree}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                      </view>

                    </view>
                  </view>
                </view>
                <!-- 变温室 -->
                <view style="width:420rpx;height:190rpx;" class="column-start-container">
                  <view style="width:420rpx;height:5rpx;" class="row-center-container">
                    <view style="width:276rpx;background: #E5E5E8;height:2rpx;"></view>
                  </view>

                  <!-- 变温室 -->
                  <view class="row-between-container" style="width:420rpx;height:88rpx;margin-top:20rpx;">
                    <view class="column-start-container" style="width:210rpx;height:88rpx;">
                      <view style="width:210rpx;" class="row-end-container">
                        <view class="row-center-container" style="height:24rpx;width:100rpx;margin-right:20rpx;">
                          <view class="temperature-content-desc">{{bwsZoneModel.bingWenModel}}</view>
                        </view>
                      </view>
                      <view style="width:210rpx;margin-top:14rpx;" class="row-end-container">
                        <view class="row-center-container" style="height:24rpx;width:100rpx;margin-right:20rpx;">
                          <text style="color: #666666;;font-size:24rpx;font-weight:400;min-width: 68rpx;">{{bwsZoneModelCurValue}}</text>
                        </view>
                      </view>
                    </view>
                    <!-- 右变温室 -->
                    <view class="column-start-container" style="width:210rpx;height:88rpx;">
                      <view style="width:210rpx;" class="row-start-container">
                        <view class="row-center-container" style="height:24rpx;width:auto;margin-left:20rpx;">
                          <view class="temperature-content-desc">{{rightBwsZoneModel.bingWenModel}}</view>
                        </view>
                      </view>
                      <view style="width:210rpx;margin-top:14rpx;" class="row-start-container">
                        <view class="row-center-container" style="height:24rpx;width:auto;margin-left:20rpx;">
                          <text style="color: #666666;;font-size:24rpx;font-weight:400;min-width: 120rpx;">{{rightBwsZoneModelCurValue}}</text>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>

              </view>
              <view wx:if="{{fridgeZoneType == 4 && !devChecking}}" class="fridge-zone-type-box-2 column-start-container">
                <!-- 冷藏室、变温室 -->
                <view class="column-start-container temperature-show-space">
                  <view style="margin-top:90rpx;" class="row-center-container temperature-show-space-row">
                    <view class="column-start-container temperature-show-space-column">
                      <view class="row-center-container temperature-show-space-column-content">
                        <view class="temperature-show-font-n">{{lcsZoneModel.bingWenModel}}</view>
                      </view>
                      <!-- 温度值 -->
                      <view style="width:209rpx;" class="row-center-container">
                        <text class="temperature-show-font-value-n">{{lcsZoneModelCurValue}}</text>
                        <image wx:if="{{lcsZoneModel.showDegree&&lcsZoneModelCurValue!='一'}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                      </view>
                    </view>
                  </view>
                </view>
                <!-- 变温室 -->
                <view style="width:420rpx;height:190rpx;" class="column-start-container">
                  <view style="width:420rpx;height:5rpx;" class="row-center-container">
                    <view style="width:276rpx;background: #E5E5E8;height:2rpx;"></view>
                  </view>
                  <view style="width:420rpx;height:30rpx;margin-top:24rpx;" class="row-center-container">
                    <view style="font-size:20rpx;color:rgba(138,138,144,0.75);font-weight:400;height:30rpx;">{{bwsZoneModel.bingWenModel}}</view>
                  </view>
                  <view style="width:420rpx;height:30rpx;margin-top:20rpx;" class="row-center-container">
                    <text style="font-size:24rpx;color: #666666;font-weight:400;width:320rpx;text-align: center;">{{bwsZoneModelCurValue}}</text>
                  </view>
                </view>
              </view>
              <view wx:if="{{fridgeZoneType == 5&& !devChecking}}" class="fridge-zone-type-box-2 column-start-container">
                <!-- 冷藏室、冷冻室 -->
                <view class="column-start-container temperature-show-space">
                  <view style="margin-top:90rpx;" class="row-start-container temperature-show-space-row">
                    <view class="row-center-container temperature-show-space-row">
                      <!-- 冷藏室 -->
                      <view class="column-center-container">
                        <view style="font-size: 24rpx;color: #7C879B;">{{lcsZoneModel.bingWenModel}}</view>
                        <view style="margin-top: 14rpx;display: flex;align-items: flex-end;min-width: 118rpx;">
                          <text style="height:80rpx;width: 70rpx;font-size: 62rpx;color: #000000;font-weight: 500;text-align: right;line-height: 80rpx;">{{lcsZoneModelCurValue}}</text>
                          <image wx:if="{{lcsZoneModel.showDegree&&lcsZoneModelCurValue!='一'}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                        </view>
                      </view>
                      <!-- 中线 -->
                      <view class="column-center-container halving-line-contain" style="margin-left: 22rpx;margin-right: 18rpx;">
                        <view class="halving-line"></view>
                      </view>
                      <!-- 冷冻室 -->
                      <view style="display: flex;align-items: flex-end;flex-direction: row;">
                        <view class="column-end-container" style="align-items: flex-end;">
                          <view style="font-size: 24rpx;color: #7C879B;">{{ldsZoneModel.bingWenModel}}</view>
                          <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:if="{{isDeepColdMode}}">{{deepcold_temp}}</text>
                          <text style="line-height: 80rpx; font-size:62rpx;color: #000000;font-weight:500;width:96rpx;height:80rpx;margin-top: 14rpx;" wx:else>{{ldsZoneModelCurValue}}</text>
                        </view>
                        <image wx:if="{{ldsZoneModel.showDegree}}" src="../card0/assets/images/degree.png" style="height: 68rpx;width: 44rpx;margin-left: 4rpx;"></image>
                      </view>

                    </view>
                  </view>
                </view>
                <!-- 右变温室 -->
                <view style="width:420rpx;height:190rpx;" class="column-start-container">
                  <view style="width:420rpx;height:5rpx;" class="row-center-container">
                    <view style="width:276rpx;background: #E5E5E8;height:2rpx;"></view>
                  </view>
                  <view style="width:420rpx;height:24rpx;margin-top:24rpx;" class="row-center-container">
                    <view style="font-size:20rpx;color:rgba(138,138,144,0.75);font-weight:400;">{{rightBwsZoneModel.bingWenModel}}</view>
                  </view>
                  <view style="width:420rpx;height:30rpx;margin-top:20rpx;" class="row-center-container">
                    <text style="font-size:24rpx;color: #666666;font-weight:400;width:320rpx;text-align:center;">{{rightBwsZoneModelCurValue}}</text>
                  </view>
                </view>
              </view>
              <!-- 冰箱自检 -->
              <view class="column-start-container" wx:if="{{devChecking}}" style="height: 460rpx;width:420rpx;">
                <view style="z-index:999;height: {{460/3}}rpx;width:420rpx;" class="column-end-container">
                  <view style="width:420rpx;" class="row-center-container">
                    <view style="color: #000000;font-size: 32rpx;font-weight:500;">
                      自检中
                    </view>
                  </view>
                </view>
                <view style="z-index:999;height: {{460/3}}rpx;width:420rpx;" class="column-center-container">
                  <view style="width: 168rpx;" class="row-start-container">
                    <view style="width: 20rpx;"></view>
                    <view style="width:116rpx;font-size: 72rpx;font-weight: 500;color: {{faultFontColor}};" class="column-center-container">
                      <view>{{checkProgress}}</view>
                    </view>
                    <view style="width:32rpx;font-weight: 600;font-size:32rpx;color: {{faultFontColor}};">
                      %
                    </view>
                  </view>
                </view>
                <view style="z-index:999;height: {{460/3}}rpx;width:420rpx;" class="column-start-container">
                  <view style="font-size:18rpx;color:#8A8A8F;font-weight:400;margin-top: 10rpx;">
                    正在检测{{checkProgressItem}}
                  </view>
                </view>
              </view>

            </view>
          </view>
          <view style="width:480rpx;height:480rpx;z-index:99;" class="column-center-container">
            <image src="{{iconServiceUrl}}ctl_bg.png" class="animation-bg-view"></image>
          </view>
          <!-- 卡券中心 -->
          <image wx:if="{{isShow == 1}}" mode="widthFix" src="{{iconServiceUrl}}xjzy_float_card.png" style="width: 128rpx;height: 128rpx;position: absolute;right: 0px;top: 0px;" bindtap="cardCenterAction"></image>
        </view>
        <!-- AI云管家 -->
        <view class="ai-out-view column-start-container" style="opacity: {{devChecking?0.6:1}};">
          <view class="row-start-container" style="width:686rpx;height:60rpx;z-index: 10000;">
            <image src="{{iconServiceUrl}}title.png" style="width:224rpx;height:60rpx;background-repeat: no-repeat;" mode="aspectFill"></image>
          </view>
          <view class="ai-out-content-view column-start-container" style="box-shadow: 0px 1px 15px 2px rgba(0,0,0,0.05);">
            <view class="ai-content-top-radius row-start-container" wx:if="{{fridgeBrand == 0 || fridgeBrand == 2}}">
              <view style="height:173rpx;width:228rpx;" class="column-center-container" bindtap="viewOpenDoorRecordAction">
                <image src="{{iconServiceUrl}}record.png" style="width:72rpx;height:72rpx;" mode="aspectFill"></image>
                <view style="color: #232323;font-size:24rpx;font-weight:600;margin-top: 10rpx;">开关门记录</view>
              </view>
              <view style="height:173rpx;width:228rpx;" class="column-center-container" bindtap="viewMonthReportAction">
                <image src="{{iconServiceUrl}}report.png" style="width:72rpx;height:72rpx;" mode="aspectFill"></image>
                <view style="color: #232323;font-size:24rpx;font-weight:600;margin-top: 10rpx;">月度报告</view>
              </view>
              <view style="height:173rpx;width:228rpx;border-top-right-radius: 24rpx;" class="column-center-container" bindtap="devCheckAction">
                <image src="{{iconServiceUrl}}check.png" style="width:72rpx;height:72rpx;" mode="aspectFill"></image>
                <view style="color: #232323;font-size:24rpx;font-weight:600;margin-top: 10rpx;">智能自检</view>
              </view>
            </view>
            <!-- colmo -->
            <view class="ai-content-top-radius row-start-container" wx:if="{{fridgeBrand == 1}}">
              <view class="row-start-container" style="height:173rpx;width: 342rpx;" bindtap="viewMonthReportAction">
                <view style="width: 64rpx;height: 64rpx;padding-left: 33rpx;padding-right: 33rpx;height:173rpx;">
                  <image src="{{iconServiceUrl}}report.png" style="width:64rpx;height:64rpx;margin-top: 48rpx;" mode="aspectFill"></image>
                </view>
                <view class="column-start-container" style="height: 173rpx;">
                  <view style="margin-top: 56rpx;width: 212rpx;height: 32rpx;" class="row-start-container">
                    <view style="font-size: 32rpx;color: #232323;font-weight: 500;">月度报告</view>
                  </view>
                  <view style="margin-top: 16rpx;width: 212rpx;height: 24rpx;" class="row-start-container">
                    <view style="font-size:24rpx;color: #8A8A8F;font-weight:400;">节能小Tips</view>
                  </view>
                </view>
              </view>
              <view style="height:173rpx;width:2rpx;" class="column-center-container">
                <view style="width:2rpx;height: 80rpx;background-color: #E5E5E8;"></view>
              </view>
              <view class="row-start-container" style="height:173rpx;width: 342rpx;" bindtap="devCheckAction">
                <view style="width: 64rpx;height: 64rpx;padding-left: 52rpx;padding-right: 34rpx;height:173rpx;">
                  <image src="{{iconServiceUrl}}check.png" style="width:64rpx;height:64rpx;margin-top: 48rpx;" mode="aspectFill"></image>
                </view>
                <view class="column-start-container" style="height: 173rpx;">
                  <view style="margin-top: 56rpx;width: 192rpx;height: 32rpx;" class="row-start-container">
                    <view style="font-size: 32rpx;color: #232323;font-weight: 500;">自检报告</view>
                  </view>
                  <view style="margin-top: 16rpx;width: 192rpx;height: 24rpx;" class="row-start-container">
                    <view style="font-size:24rpx;color: #8A8A8F;font-weight:400;">智能检查</view>
                  </view>
                </view>
              </view>
            </view>

            <!-- 分割线 -->
            <view style="width:686rpx;height:4rpx;" class="column-center-container">
              <view style="background:#F2F2F2;height:2rpx;" class="container-common-width"></view>
            </view>

            <!-- AI云管家 智能保鲜 -->
            <view style="height:141rpx;width:686rpx;" class="ai-content-bottom-radius column-center-container">
              <view class="row-between-container" style="width:626rpx;padding-left:30rpx;padding-right:30rpx;">
                <view class="row-start-container" style="width:526rpx;">
                  <view style="width:64rpx;height:64rpx;background:#267AFF;border-radius:50%;" class="column-center-container">
                    <image src="{{iconServiceUrl}}zhinengbaoxian-m.png" style="width:46rpx;height:46rpx;" mode="aspectFill"></image>
                  </view>
                  <view style="font-size:32rpx;color:#000000;font-weight: 400;margin-left:34rpx;">智能保鲜</view>
                </view>
                <view style="width:100rpx;" class="row-end-container">
                  <view class="row-end-container ai-opt-ctr-switch-open-out" wx:if="{{cloudKeeperSwitchStatus}}" bindtap="cloudKeeperSwitchStatusAction">
                    <view class="ai-opt-ctr-switch-open-in"></view>
                  </view>

                  <view style="width:88rpx;height:40rpx;border-radius:24rpx;border:4rpx solid #8A8A8F;" class="row-start-container" wx:if="{{!cloudKeeperSwitchStatus}}" bindtap="cloudKeeperSwitchStatusAction">
                    <view class="ai-opt-ctr-switch-close-in"></view>
                  </view>
                </view>
              </view>
            </view>

          </view>
        </view>
        <!-- 工作模式 -->
        <view style="width:686rpx;margin-top:30rpx;background:#FFFFFF;border-radius:16rpx;padding-top:10rpx;padding-bottom:10rpx;opacity: {{devChecking?0.6:1}};box-shadow: 0px 1px 15px 2px rgba(0,0,0,0.05);" class="column-start-container">
          <!-- //工作模式开始 -->
          <mode-panel id="model-container" bindmodeUpdate="modeUpdate"></mode-panel>
          <!-- //工作模式结束 -->
          <!-- 温度调节 -->
          <view style="height:144rpx;width:686rpx;" class="column-center-container" bindtap="extendViewAction">
            <view class="row-between-container" style="width:626rpx;padding-left:30rpx;padding-right:30rpx;">
              <view class="row-start-container" style="width:526rpx;">
                <view style="width:64rpx;height:64rpx;background:#267AFF;border-radius:50%;" class="column-center-container">
                  <image src="{{iconServiceUrl}}wendu-m.png" style="width:46rpx;height:46rpx;" mode="aspectFill"></image>
                </view>
                <view style="font-size:32rpx;color:#000000;font-weight: 400;margin-left:34rpx;">温度调节</view>
              </view>
              <view style="width:100rpx;" class="row-end-container">
                <view style="width:24rpx;height:64rpx;" class="column-center-container">
