/* plugin/T0xCA/guide/index.wxss */
@import "../card0/assets/css/common.wxss";

page {
  background-color: #F9F9F9;
}

.page-card {
  top: 280rpx;
  position: absolute;
  z-index: 99999;
  padding-bottom: 25px;
}

.page-card-content {
  background: #FFFFFF;
  box-shadow: 0px 0px 12px 0px rgba(0,0,0,0.06);
  border-radius: 24rpx;
}

.finish-btn {
  width: 60px;
  height: 24px;
  margin-right: 12px;
  background: #267AFF;
  border-radius: 12px;
  font-size: 24rpx;
  color: #FFFFFF;
  font-weight: 400;
}

.header {
  height: 60rpx;
  width: 100%;
  padding-left: 32rpx;
  padding-right: 32rpx;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 60rpx;
  font-size: 18px;
  white-space: nowrap;
  top: 44rpx;
  left: 0;
  z-index: 99999;
  display: flex;
  align-items: center;
}

.logo{
    display: inline-block;
    width: 56rpx;
    height: 56rpx;
    margin-right: 18rpx;
}
.back{
  width: 52rpx;
  height: 52rpx;
  display: inline-block;
}

.nav-title {
  font-size:32rpx;
  color: #000000;
  font-weight:500;
}

.nav-title-container{
  width: 100%;
  height: 60rpx;
}

.header-new {
  width: 100%;
  position: fixed;
  top: 0rpx;
  left: 0;
  z-index: 9999999;
  display: flex;
  align-items: center;
  background-color: #F9F9F9;
}
