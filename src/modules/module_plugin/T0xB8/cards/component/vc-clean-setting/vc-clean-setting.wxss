.clean-setting-container{
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin: 0 32rpx 2vh;
}
.clean-state-container{
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    height: 84px;
}
.clean-setting-layout{
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
.clean-setting-title{
    font-size: 16px;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #000000;
    margin-left: 32rpx;
}
.clean-mode-layout{
    position: relative;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    margin-top: 24rpx;
}
.lastest-order-desc{
    font-size: 14px;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #8A8A8F;
    margin-left: 32rpx;
    margin-top: 24rpx;
}
.imageStyle{
    position: absolute;
    left:0px;
    top:0px;
    width:238px;
    height:238px;
}
.cleanStateStyle{
    position:relative;
    margin-top:95px;
    display: flex;
    align-items: center;
    justify-content: center; 
    font-size: 36px;
}
.batteryStyle{
    position:relative;
    top:5px;
    display: flex;
    justify-content: center; 
}

.left{
    width: 43vw;
    height: 18vw;
    background: #FFFFFF;
    box-shadow: 0px 1px 15px 2px rgba(0, 0, 0, 0.05);
    border-radius: 8px;
    filter: blur(0px);
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}
.right{
    width: 43vw;
    height: 18vw;
    background: #FFFFFF;
    box-shadow: 0px 1px 15px 2px rgba(0, 0, 0, 0.05);
    border-radius: 8px;
    filter: blur(0px);
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}
.fan-icon,.water-icon{
    width: 9vw;
    height: 9vw;
    margin-left: 9vw;
}
.arrow{
    width: 7vw;
    height: 7vw;
    margin-right: 2vw;
}
.text{
    margin-left: 4vw;
}
