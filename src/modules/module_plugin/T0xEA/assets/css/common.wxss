.click:active{
    opacity: .6;
}
.center-flex{
    display: flex;
    justify-content: center;
    align-items: center;
}
.start-flex{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.between-flex{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.transition-default{
    transition: all ease-in-out .3s;
}
.fade-out{
    opacity: 0;
}
.fade-in{
    opacity: 1;
}
