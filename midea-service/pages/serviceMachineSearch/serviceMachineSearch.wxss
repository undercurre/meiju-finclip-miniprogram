/* midea-service//pages/serviceMachineSearch/serviceMachineSearch.wxss */
.page {
  background-color: #f9f9f9;
  font-family: PingFangSC-Medium;
}

page {
  background-color: #f9f9f9;
}
.search {
  width: 100%;
  padding: 24rpx 32rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.search .content {
  position: relative;
  padding-right: 88rpx;
  background: #f6f6f6;
  border-radius: 36px;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
}

.search-ic {
  width: 22px;
  height: 22px;
  margin-left: 10rpx;
  /* scale: 0.6; */
  /* position: absolute;
    left: 35rpx;
    top: 30rpx; */
}

.search-input {
  width: 83%;
  height: 72rpx;
  background: #f6f6f6;
  border-radius: 36rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #000000;
  line-height: 28rpx;
  font-weight: 500;
}

.placeholder {
  font-size: 28rpx;
  color: #8a8a8f;
  line-height: 28rpx;
  font-weight: 500;
}

.reset {
  width: 22px;
  height: 22px;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 20rpx;
}

.model-list {
  padding-left: 32rpx;
  background-color: #fff;
}

.model {
  height: 104rpx;
  font-size: 32rpx;
  color: #000000;
  line-height: 104rpx;
  font-weight: 400;
}

.bor-bottom {
  border-bottom: 1px solid #f2f2f2;
}

.null {
  display: flex;
  /* justify-content: center; */
  flex-direction: column;
  align-items: center;
}

.null-img {
  width: 480rpx;
  height: 400rpx;
  display: block;
}

.null-desc {
  margin-top: 44rpx;
  text-align: center;
  font-size: 28rpx;
  color: #8a8a8f;
  letter-spacing: 0;
  line-height: 36rpx;
  font-weight: 400;
}

.product-text{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}