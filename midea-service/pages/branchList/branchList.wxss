.container-box {
  display: flex;
  flex-direction: column;
  background-color: #f6f6f6;
  font-family: PingFangSC-Regular;
  height: 100vh;
  overflow: hidden;
  box-sizing: border-box;
}

.padding-inner {
  width: 100%;
}

.no-distance {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}

.top-bg {
  width: 480rpx;
  height: 400rpx;
  margin-top: 8rpx;
}

.n-title {
  margin-top: 40rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: center;
  line-height: 36rpx;
  font-weight: 400;
}

.t-content {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #666666;
  letter-spacing: 0;
  text-align: center;
  line-height: 36rpx;
  font-weight: 400;
}

.mt48 {
  margin-top: 48rpx;
}

.t-content0 {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #666666;
  letter-spacing: 0;
  line-height: 40rpx;
  text-align: center;
  font-weight: 400;
}
.btn-box {
  position: relative;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #267aff;
  letter-spacing: 0;
  text-align: center;
  line-height: 32rpx;
  font-weight: 400;
  margin-top: 40rpx;
}
.btn-box-cilck {
  position: absolute;
  width: 120%;
  height: 150%;
  top: -10rpx;
  right: -10rpx;
  bottom: -10rpx;
  left: -10rpx;
}
.btn-boxs {
  width: 240rpx;
  height: 72rpx;
  margin-top: 56rpx;
  background: #267aff;
  border-radius: 36rpx;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  line-height: 72rpx;
  font-weight: 500;
}

.fixed-header {
  padding: 0 32rpx;
  height: 104rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.commonHeight {
  height: 52rpx;
  line-height: 52rpx;
}

.left-check-box {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  border-right: 1rpx solid #f2f2f2;
  margin-right: 22rpx;
}

.address-check {
  max-width: 250rpx;
  min-width: 139rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #000000;
  font-weight: 500;
}

.jiantou {
  width: 20rpx;
  height: 20rpx;
  margin: 0 20rpx 0 9rpx;
}

.right-c {
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #000000;
  font-weight: 400;
}

.import-content {
  display: inline-block;
  vertical-align: top;
  color: #ff8f00;
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #ff8f00;
  font-weight: 500;
}

.one-item-box {
  padding: 32rpx;
  margin-bottom: 16rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.one-item-box:nth-child(1) {
  margin-top: 16rpx;
}
.one-item-box .left-title-content {
  flex: 1;
  padding-right: 40rpx;
}

.left-title-content .top-title {
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #000000;
  font-weight: 500;
  margin-bottom: 16rpx;
}

.center-content {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #8a8a8f;
  line-height: 32rpx;
  font-weight: 400;
  margin-bottom: 32rpx;
}

.bottom-content {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.b-left {
  height: 24rpx;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #000000;
  line-height: 24rpx;
  font-weight: 500;
  border-right: 1rpx solid #8a8a8f;
  text-align: left;
  padding-right: 8rpx;
  margin-right: 8rpx;
}

.b-left text {
  display: inline-block;
  vertical-align: top;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #000000;
  text-align: center;
  line-height: 24rpx;
  font-weight: 500;
}

.b-right {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #8a8a8f;
  line-height: 32rpx;
  font-weight: 400;
}

.right-phone-map {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.r-l-icon {
  width: 58rpx;
  height: 58rpx;
  display: block;
  margin: 0 auto;
}

.r-l-icon-l {
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #666666;
  line-height: 20rpx;
  font-weight: 400;
  height: 20rpx;
  text-align: center;
  margin-top: 10rpx;
}

.right-l {
  margin-right: 34rpx;
  height: 88rpx;
}

.right-r {
  height: 88rpx;
}
.line-1 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  line-clamp: 1;
}
.sit-address {
  padding-bottom: 60rpx;
}
.no-system-auth {
  padding: 0 136rpx;
  word-wrap: normal;
}
