.fault .book-body {
  position: absolute;
  z-index: 11;
  background: #fff;
  border-radius: 4px;
  width: 80%;
  height: 172px;
  top: 50%;
  left: 10%;
  color: #333;
}

.fault .book-body .book-title {
  text-align: center;
  height: 20px;
  line-height: 20px;
  font-size: 16px;
  margin: 20px 0 8px;
}

.fault .book-body .book-spec {
  padding: 8px 18px;
  font-size: 12px;
  color: #999;
  border-top: 1px solid #ddd6;
}

.fault .book-body .book-btn {
  position: absolute;
  bottom: 10px;
  left: 6%;
  width: 88%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 16px;
  color: white;
  background: #318bf5;
  border-radius: 4px;
}

.fault .fl-body {
  position: fixed;
  z-index: 11;
  background: #fff;
  border-radius: 4px;
  width: 80%;
  left: 10%;
  top: 20%;
}

.fault .fl-body .fl-item {
  padding-top: 8px;
}

.fault .fl-body .fl-item .fl-size {
  height: 114px;
  overflow-y: auto;
  border-bottom: 1px solid #efefef;
  padding: 0 18px;
}

.fault .fl-body .fl-item .img {
  width: 18px !important;
  height: 18px !important;
  color: #030303;
  margin-right: 10px;
  margin-bottom: -3px;
}

.fault .fl-body .fl-item .fl-title {
  margin-top: 10px;
  font-size: 17px;
}

.fault .fl-body .fl-item .p {
  color: #999;
  font-size: 15px;
  padding-bottom: 10px;
}

.fault .fl-body .fl-item .fl-action {
  display: flex;
  justify-content: space-between;
  padding: 10px 6%;
}

.fault .fl-body .fl-item .fl-action .fl-action-left,
.fault .fl-body .fl-item .fl-action .fl-action-right {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 47%;
  height: 40px;
  line-height: 40px;
  color: #999;
  border-radius: 4px;
  color: #ccc;
  font-size: 16px;
  border: 1px solid #ccc;
}

.fl-item .fl-action .iconfont {
  font-size: 20px;
  align-self: center;
  padding-right: 10px;
}

.icon-help {
  width: 16px;
  height: 16px;
}

.fault .fl-body .fl-item .fl-action .fl-action-left.selected {
  color: #ff9800;
  border: 1px solid #ff9800;
}

.fault .fl-body .fl-item .fl-action .fl-action-right.selected {
  color: #318bf5;
  border: 1px solid #318bf5;
}

.fault .fl-body .fl-know {
  position: absolute;
  bottom: 0;
  border-top: 1px solid #efefef;
  text-align: center;
  color: #318bf5;
  font-size: 15px;
  line-height: 40px;
  width: 100%;
  left: 0;
}

.swiper-wrap {
  position: relative;
}

.swiper-wrap .swiper {
  height: 296px;
}

.swiper-wrap .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 70px;
  display: flex;
  justify-content: center;
}

.swiper-wrap .dots .dot {
  margin: 0 4px;
  width: 6px;
  height: 6px;
  background: #d0cdd1;
  border-radius: 50%;
  transition: all 0.6s;
}

.swiper-wrap .dots .dot.active {
  background: #318bf5;
}

