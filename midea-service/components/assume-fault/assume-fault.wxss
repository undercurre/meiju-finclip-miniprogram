.loading-box {
  flex: 1;
  padding: 0 15px;
}

.loading-cell {
  background: #f2f2f2;
  height: 20px;
  margin-top: 10px;
  position: relative;
  overflow: hidden;
}

.loading-cell::after {
  content: '';
  position: absolute;
  display: block;
  top: -10%;
  left: 0;
  width: 20px;
  height: 200%;
  transform: rotate(30deg);
  transform-origin: top;
  opacity: 0.2;
  background-image: linear-gradient(to right, #ffffff 0%, #f2f2f2 100%);
  animation: loadingCell 1.5s ease infinite forwards;
}

.loading-title {
  width: 100px;
  margin-top: 25px;
}

.loading-des-long {
  height: 20px;
}

.loading-des-short {
  width: 80%;
}

.loading-img {
  height: 159px;
  margin-top: 10px;
  background: #f2f2f2;
  position: relative;
  overflow: hidden;
}

.loading-img::after {
  content: '';
  position: absolute;
  display: block;
  top: -10%;
  left: 0;
  width: 50px;
  height: 200%;
  transform: rotate(30deg);
  transform-origin: top;
  opacity: 0.2;
}
::-webkit-scrollbar {
  width: 0;
